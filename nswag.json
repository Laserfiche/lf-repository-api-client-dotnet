{
  "runtime": "Default",
  "defaultVariables": null,
  "documentGenerator": {
    "fromDocument": {
      "url": "swagger.json",
      "output": null,
      "newLineBehavior": "Auto"
    }
  },
  "codeGenerators": {
    "openApiToCSharpClient": {
      "classname": "{controller}Client",
      "clientBaseClass": "BaseClient",
      "namespace": "Laserfiche.Repository.Api.Client",
      "output": "src/Clients/RepositoryClients.cs",
      "operationGenerationMode": "MultipleClientsFromFirstTagAndOperationId",
      "additionalNamespaceUsages": [ "Laserfiche.Api.Client" ],
      "excludedTypeNames": [ "ProblemDetails" ],
      "generateClientInterfaces": true,
      "generateExceptionClasses": false,
      "generateOptionalParameters": true,
      "generateUpdateJsonSerializerSettingsMethod": false,
      "useBaseUrl": false,
      "clientClassAccessModifier": "internal",
      "templateDirectory": "nswag",
      "wrapResponses": true,
      "wrapResponseMethods": [ "EntriesClient.GetDocumentContentType" ],
      "responseClass": "HttpResponseHead"
    }
  }
}